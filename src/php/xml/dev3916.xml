                <device>0039-12-02-C</device>
                <conpin>PB1</conpin>
            </connect>
        </Pins>
        <Pins>
            <name>Port2</name>
            <function>DO</function>
            <connect>
                <device>0039-12-02-C</device>
                <conpin>PB3</conpin>
            </connect>
        </Pins>
        <Pins>
            <name>Port3</name>
            <function>DO</function>
            <connect>
                <device>0039-12-02-C</device>
                <conpin>AREF</conpin>
            </connect>
